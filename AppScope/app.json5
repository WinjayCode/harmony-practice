// app.json5是应用的全局的配置文件，用于存放应用公共的配置信息。
{
  "app": {
    // 包名
    "bundleName": "com.winjay.practice",
    // 应用程序供应商
    "vendor": "Winjay",
    // 用于区分应用版本
    "versionCode": 1000000,
    // 版本号
    "versionName": "1.0.0",
    // 应用的显示图标
    "icon": "$media:app_icon",
    // 应用名
    "label": "$string:app_name"
  }
}
